<?xml version="1.0" encoding="UTF-8"?>
<LangFile>
    <!-- Activation du mode developpement, forcément être activé pour utiliser les ligne suivantes
        Value: true, false -->
    <entry key="dev.mode">true</entry>

    <!-- Affiche en jeu la mini interface affichant le fps et les accés rapides
        Value: true, false -->
    <entry key="dev.auto.display.controler">true</entry>

    <!-- Affiche la fenêtre listant les événements du jeu
        Value: true, false -->
    <entry key="dev.auto.display.eventUtil">true</entry>

    <!-- Affiche l'utilitaire pour créer ou jouer des scripts Lua
        Value: true, false -->
    <entry key="dev.auto.display.luaUtil">true</entry>

    <!-- Activation de la console
        Value: true, false -->
    <entry key="boo">true</entry>

    <!-- Changer le build type du client
        Value: debug, internal, testing, alpha, beta, release -->
    <entry key="buildType">debug</entry>
</LangFile>
